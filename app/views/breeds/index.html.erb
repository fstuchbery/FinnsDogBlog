<h1>All Breeds</h1>
<%= link_to "Home Page", homes_path %>
<%= link_to "About Us", about_path %>
<%= link_to "Dog Breeds", breeds_path %>

<table>
  <thead>
    <tr>
    <th>ID</th>  
    <th>Name</th>
      
    </tr>
  </thead>
  <tbody>
    <% @breeds.each do |breed| %>
      <tr>
        <td><%= breed.id %></td>
        <td><%= breed.name %></td>
        
      </tr>
    <% end %>
  </tbody>
</table>
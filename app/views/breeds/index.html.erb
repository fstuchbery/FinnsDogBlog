<h1>All Breeds</h1>
<%= link_to "Home Page", articles_path %>
<%= link_to "About Us", about_path %>
<%= link_to "Dog Breeds", breeds_path %>

<table>
  <thead>
    <tr>
      <th>Name</th>
    </tr>
  </thead>
  <tbody>
    <% @breeds.each do |breed| %>
      <tr>
      
        <td><%= breed.name %></td>
        <td><%= breed.id %></td>
        </tr>
    <% end %>
  </tbody>
</table>
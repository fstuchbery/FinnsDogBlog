<body class = "bodyy">
<h1>Owner Page</h1>
<%= link_to "Home", homes_path %>
<%= link_to "About", about_path %>
<%= link_to "Dog Breeds", breeds_path %>


<h1>Owner: <%= @owner.name %>  age: <%= @owner.age %> </h1>
<h2>Dogs:</h2>
<ul>
  <% @owner.dogs.each do |dog| %>
    <li>
    <%= link_to dog.name, dog_path(dog) %> (Breed: <%= dog.breed.name if dog.breed %>)
    </li>
  <% end %>
</ul>
</body>